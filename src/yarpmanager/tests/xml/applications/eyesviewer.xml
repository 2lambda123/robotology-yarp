<application>
    <name>EyesViewer</name>
    <description>A fake robot eyes viewer running on a cluster of computer</description>
    <version>1.0</version>
    <authors>
        <author email="ali.paikan@iit.it">Ali Paikan</author>
    </authors>
    <module>
        <name>activeSeg</name>
        <parameters></parameters>
        <node></node>
        <prefix>/EyesViewer:activeSeg:1</prefix>
        <geometry>(Pos (x 800) (y 593.9))</geometry>
    </module>
    <module>
        <name>test_grabber</name>
        <parameters></parameters>
        <node></node>
        <prefix>/EyesViewer:test_grabber:1</prefix>
        <deployer>yarpdev</deployer>
        <geometry>(Pos (x 1069) (y 617.9))</geometry>
    </module>
    <module>
        <name>yarpview</name>
        <parameters></parameters>
        <node></node>
        <prefix>/EyesViewer:yarpview:1</prefix>
        <geometry>(Pos (x 1183) (y 698.9))</geometry>
    </module>
    <connection>
        <from>/EyesViewer:activeSeg:1/activeSeg/imageCrop:o</from>
        <to>/EyesViewer:yarpview:1/yarpview/img:i</to>
        <protocol>udp</protocol>
        <geometry>(Pos ((x 1064) (y 683)) ((x 944) (y 649)) ((x 1184) (y 717))  )</geometry>
    </connection>
    <connection>
        <from>/EyesViewer:activeSeg:1/activeSeg/image:o</from>
        <to>/EyesViewer:test_grabber:1/test_grabber/quit</to>
        <protocol>udp</protocol>
        <geometry>(Pos ((x 1007) (y 630)) ((x 944) (y 624)) ((x 1070) (y 636))  )</geometry>
    </connection>
    <connection>
        <from>Source</from>
        <to>Destination</to>
        <protocol></protocol>
        <geometry>(Pos ((x 920.5) (y 543.5)) ((x 774) (y 538)) ((x 1067) (y 549))  )</geometry>
    </connection>
</application>
