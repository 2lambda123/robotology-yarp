<application>
    <name>EyesViewer</name>
    <description>A fake robot eyes viewer running on a cluster of computer</description>
    <version>1.0</version>
    <authors>
        <author email="ali.paikan@iit.it">Ali Paikan</author>
    </authors>
    <module>
        <name>lumaChroma</name>
        <parameters></parameters>
        <node></node>
        <prefix>/EyesViewer:lumaChroma:1</prefix>
        <geometry>(Pos (x 687) (y 141.9))</geometry>
    </module>
    <module>
        <name>test_grabber</name>
        <parameters></parameters>
        <node></node>
        <prefix>/EyesViewer:test_grabber:1</prefix>
        <deployer>yarpdev</deployer>
        <geometry>(Pos (x 459) (y 192.9))</geometry>
    </module>
    <module>
        <name>yarpview</name>
        <parameters></parameters>
        <node></node>
        <prefix>/EyesViewer:yarpview:1</prefix>
        <geometry>(Pos (x 936) (y 141.9))</geometry>
    </module>
    <connection>
        <from>/EyesViewer:test_grabber:1/grabber</from>
        <to>/EyesViewer:lumaChroma:1/lumaChroma/image:i</to>
        <protocol>udp</protocol>
        <geometry>(Pos ((x 657.5) (y 210.5)) ((x 627) (y 211)) ((x 688) (y 210))  )</geometry>
    </connection>
    <connection>
        <from>/EyesViewer:lumaChroma:1/lumaChroma/Y/image:o</from>
        <to>/EyesViewer:yarpview:1/yarpview/img:i</to>
        <protocol>udp</protocol>
        <geometry>(Pos ((x 888) (y 160)) ((x 839) (y 160)) ((x 937) (y 160))  )</geometry>
    </connection>
</application>
