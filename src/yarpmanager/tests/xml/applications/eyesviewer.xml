<application>
    <name>EyesViewer</name>
    <description>A fake robot eyes viewer running on a cluster of computer</description>
    <version>1.0</version>
    <authors>
        <author email="ali.paikan@iit.it">Ali Paikan</author>
    </authors>
    <module>
        <name>activeSeg</name>
        <parameters></parameters>
        <node></node>
        <prefix></prefix>
        <geometry>(Pos (x 235) (y 322.9))</geometry>
    </module>
    <module>
        <name>yarpscope</name>
        <parameters></parameters>
        <node></node>
        <prefix>/EyesViewer:yarpscope:1</prefix>
        <geometry>(Pos (x 601) (y 426.9))</geometry>
    </module>
    <module>
        <name>yarpview</name>
        <parameters></parameters>
        <node></node>
        <prefix>/EyesViewer:yarpview:2</prefix>
        <geometry>(Pos (x 597) (y 334.9))</geometry>
    </module>
    <connection>
        <from>/activeSeg/image:o</from>
        <to>/EyesViewer:yarpview:2/yarpview/img:i</to>
        <protocol>udp</protocol>
        <geometry>(Pos ((x 488.5) (y 353)) ((x 379) (y 353)) ((x 598) (y 353))  )</geometry>
    </connection>
    <connection>
        <from>/activeSeg/imageCrop:o</from>
        <to>/EyesViewer:yarpscope:1/yarpscope</to>
        <protocol>udp</protocol>
        <geometry>(Pos ((x 512) (y 411.5)) ((x 379) (y 378)) ((x 512) (y 378)) ((x 512) (y 445)) ((x 602) (y 445))  )</geometry>
    </connection>
</application>
