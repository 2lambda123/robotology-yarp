<application>
    <name>Efficient_graph-based_segmentation</name>
    <module>
        <name>graphBasedSegmentation</name>
        <parameters>--name GBSeg</parameters>
        <node>node1</node>
        <prefix>GBseg</prefix>
        <geometry>(Pos (x 128) (y 98.9))</geometry>
    </module>
    <module>
        <name>yarpview</name>
        <parameters>--name /segView --x 0 --y 0 --RefreshTime 33</parameters>
        <node>node2</node>
        <prefix>segView</prefix>
        <geometry>(Pos (x 435) (y 103.9))</geometry>
    </module>
    <module>
        <name>yarpview</name>
        <parameters>--name /original --x 330 --y 0 --out /desiredTrack --RefreshTime 33</parameters>
        <node>node2</node>
        <prefix>original</prefix>        
    </module>
    <connection>
        <from>/icub/camcalib/left/out</from>
        <to>/GBSeg/rawImg:i</to>
        <protocol></protocol>        
    </connection>
    <connection>
        <from>/GBSeg/viewImg:o</from>
        <to>/segView</to>
        <protocol></protocol>        
    </connection>
    <connection>
        <from>/icub/camcalib/left/out</from>
        <to>/original</to>
        <protocol></protocol>        
    </connection>
</application>
