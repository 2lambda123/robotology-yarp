<application>
    <name>EyesViewer-Cluster</name>
    <description>A fake robot eyes viewer running on a cluster of computer</description>
    <version>1.0</version>
    <authors>
        <author email="ali.paikan@iit.it">Ali Paikan</author>
    </authors>
    <module>
        <name>yarpdev</name>
        <parameters>--device test_grabber --name /left/eye</parameters>
        <node></node>
        <prefix></prefix>
        <geometry>(Pos (x 186.9) (y 51.9))</geometry>
    </module>
    <module>
        <name>yarpdev</name>
        <parameters>--device test_grabber --name /right/eye</parameters>
        <node></node>
        <prefix></prefix>
        <geometry>(Pos (x 14.9) (y 51.9))</geometry>
    </module>
    <module>
        <name>yarpview</name>
        <parameters></parameters>
        <node></node>
        <prefix>/RightView</prefix>
        <geometry>(Pos (x 211) (y 182.9))</geometry>
    </module>
    <module>
        <name>yarpview</name>
        <parameters></parameters>
        <node></node>
        <prefix>/LeftView</prefix>
        <geometry>(Pos (x 212) (y 142.9))</geometry>
    </module>
    <connection>
        <from>/left/eye</from>
        <to>/LeftView/yarpview/img:i</to>
        <protocol>UDP</protocol>
        <geometry>(Pos ((x 103) (y 161)) ((x 213) (y 161))  )</geometry>
    </connection>
    <connection>
        <from>/right/eye</from>
        <to>/RightView/yarpview/img:i</to>
        <protocol>UDP</protocol>
        <geometry>(Pos ((x 110) (y 201)) ((x 212) (y 201))  )</geometry>
    </connection>
</application>
