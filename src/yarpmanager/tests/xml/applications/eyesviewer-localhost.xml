<application>
    <name>EyesViewer-Localhost</name>
    <description>A fake robot eyes viewer with integrated applications running on localhost</description>
    <version>1.0</version>
    <authors>
        <author email="ali.paikan@iit.it">Ali Paikan</author>
    </authors>
    <module>
        <name>yarpview</name>
        <parameters></parameters>
        <node>localhost</node>
        <prefix>/RightView</prefix>
        <geometry>(Pos (x 215) (y 167.6))</geometry>
    </module>
    <module>
        <name>yarpview</name>
        <parameters></parameters>
        <node>localhost</node>
        <prefix>/LeftView</prefix>
        <geometry>(Pos (x 216) (y 112.6))</geometry>
    </module>
    <application>
        <name>FakeEyes</name>
        <prefix>/left</prefix>
    </application>
    <application>
        <name>FakeEyes</name>
        <prefix>/right</prefix>
    </application>
    <connection>
        <from>/left/eye</from>
        <to>/LeftView/yarpview/img:i</to>
        <protocol>UDP</protocol>
        <geometry>(Pos ((x 108) (y 130)) ((x 217) (y 130))  )</geometry>
    </connection>
    <connection>
        <from>/right/eye</from>
        <to>/RightView/yarpview/img:i</to>
        <protocol>UDP</protocol>
        <geometry>(Pos ((x 113) (y 185)) ((x 216) (y 185))  )</geometry>
    </connection>
</application>
