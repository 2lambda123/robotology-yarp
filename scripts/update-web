#!/bin/bash

# run as "./scripts/update-web"

echo Updating Sourceforge website from local system

target="yarp0.sourceforge.net"

USER=`cat CVS/Root | sed "s/^:[^:]*://" | sed "s/[^a-z].*//"`

if [ ! "k$USER" = "k" ] ; then

    echo USER might be $USER

    cd doc && rsync --rsh="ssh -x -l $USER" --modify-window=2 -lauvzP . $target:www/specs/dox

else

    echo cannot figure out CVS username

fi



