// STUB FOR CARRIER

#include <stdio.h>
void add_owned_@YARPDEV_NAME@(const char *owner) {
    if (owner==NULL) {
        fprintf(stderr,"Cannot add stub for device @YARPDEV_NAME@\n");
        return;
    }
    printf("Should add stub for carrier @YARPDEV_NAME@\n");
}

void add_@YARPDEV_NAME@() {
    add_owned_@YARPDEV_NAME@(NULL);
}

