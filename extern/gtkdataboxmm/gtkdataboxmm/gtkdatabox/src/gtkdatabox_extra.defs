;; From GtkDatabox

(define-signal zoomed
  (of-object "GtkDatabox")
  (return-type "void")
  (when "first")
)

(define-signal selection-started
  (of-object "GtkDatabox")
  (return-type "void")
  (when "first")
  (parameters
    '("gpointer" "p0")
  )
)

(define-signal selection-changed
  (of-object "GtkDatabox")
  (return-type "void")
  (when "first")
  (parameters
    '("gpointer" "p0")
  )
)

(define-signal selection-finalized
  (of-object "GtkDatabox")
  (return-type "void")
  (when "first")
  (parameters
    '("gpointer" "p0")
  )
)

(define-signal selection-canceled
  (of-object "GtkDatabox")
  (return-type "void")
  (when "first")
)

(define-property enable-selection
  (of-object "GtkDatabox")
